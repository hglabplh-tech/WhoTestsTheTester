<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:csvxml="hglabplh-tech.github.io/csvxml"
           targetNamespace="hglabplh-tech.github.io/csvxml"
           xmlns="http://hglabplh-tech.github.io"
           elementFormDefault="qualified">

    <xs:element name="linekeyvals">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="line" type="csvxml:rowdev" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="kvdef">
        <xs:sequence minOccurs="1" maxOccurs="unbounded">
            <xs:element name="key" type="xs:string"  minOccurs="1" maxOccurs="1"/>
            <xs:element name="value" type="xs:string" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="row" id="r1">
        <xs:sequence>
            <xs:element name="line" type="csvxml:rowdev" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="rowdev">
        <xs:sequence>
            <xs:element name="keyval" type="csvxml:kvdef" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>


</xs:schema>